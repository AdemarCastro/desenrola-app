{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -L --watch src --ext ts,js,json --exec \"ts-node --project tsconfig.json src/server.ts\"",
    "start:dev": "npm run migrate:deploy:dev && npm run seed:dev && npm run dev",
    "deploy": "npm run generate:prod && npm run build && npm run migrate:deploy && npm run seed:prod && node dist/src/server.js",
    "migrate:dev": "prisma migrate dev --schema=prisma/sqlite/schema.prisma",
    "migrate:deploy:dev": "prisma migrate deploy --schema=prisma/sqlite/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=prisma/postgres/schema.prisma",
    "migrate:reset": "prisma migrate reset",
    "migrate:create:dev": "prisma migrate dev --schema=prisma/sqlite/schema.prisma --name",
    "migrate:create:deploy": "prisma migrate dev --schema=prisma/postgres/schema.prisma",
    "seed:dev": "prisma db seed --schema=prisma/sqlite/schema.prisma",
    "seed:prod": "prisma db seed --schema=prisma/postgres/schema.prisma",
    "generate:dev": "prisma generate --schema=prisma/sqlite/schema.prisma",
    "generate:prod": "prisma generate --schema=prisma/postgres/schema.prisma",
    "debug:structure": "echo '=== Pasta dist/ ===' && ls -R dist && echo '=== Conte√∫do de src/ ===' && ls -R src",
    "studio": "prisma studio",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint \"src/**/*.{ts,js}\" --fix",
    "test": "jest"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "backend": "file:",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.7.0",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ms": "^0.7.32",
    "@types/node": "^22.15.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}
