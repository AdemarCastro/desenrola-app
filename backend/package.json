{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "deploy": "npm run generate:prod && npm run build && npm run debug:structure && npm run migrate:create:deploy && npm run migrate:deploy && npm run seed:prod && node dist/src/server.js",
    "migrate:dev": "prisma migrate dev --schema=prisma/sqlite/schema.prisma",
    "migrate:deploy:dev": "prisma migrate deploy --schema=prisma/sqlite/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=prisma/postgres/schema.prisma",
    "migrate:reset": "prisma migrate reset",
    "migrate:create:dev": "prisma migrate dev --schema=prisma/sqlite/schema.prisma --name",
    "migrate:create:deploy": "prisma migrate deploy --schema=prisma/postgres/schema.prisma",
    "seed:dev": "prisma db dev --schema=prisma/sqlite/schema.prisma",
    "seed:prod": "prisma db prod --schema=prisma/postgres/schema.prisma",
    "generate:dev": "prisma generate --schema=prisma/sqlite/schema.prisma",
    "generate:prod": "prisma generate --schema=prisma/postgres/schema.prisma",
    "debug:structure": "echo '=== Pasta dist/ ===' && ls -R dist && echo '=== Conte√∫do de src/ ===' && ls -R src",
    "studio": "prisma studio",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint \"src/**/*.{ts,js}\" --fix",
    "test": "jest"
  },
  "prisma": {
    "dev": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "prod": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^5.1.0",
    "prisma": "^6.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.3",
    "eslint": "^9.26.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  }
}
